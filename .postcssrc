{
  "plugins": {
    "stylelint": {
      "extends": [
        "stylelint-config-standard",
        "stylelint-config-standard-scss",
        "stylelint-config-prettier-scss"
      ],
      "rules": {
        "selector-class-pattern": null,
        "rule-empty-line-before": null
      },
      "cache": true
    },
    "postcss-reporter": {
      "clearReportedMessages": true
    }
  }
}
